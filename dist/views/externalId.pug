doctype html
html
  head
    title set external ids
  body
    h1 File Upload
    .foldable-container
      form(action='/upload', method='POST', enctype='multipart/form-data')
        label(for='file') Choose a file:
        input(type='file', id='file', name='file')
        button(type='submit') Upload
    h1 Example tabel
table
  tr
    th email
    th externalId
  tr
    td memyself@example.com
    td 30
  tr
    td hehimself@example.com
    td 25
